{
  "name": "monportfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "docker run --rm --name ui -e NODE_ENV=local -d -p 80:80 ui:latest && ng serve -o --disable-host-check=true --host=0.0.0.0 &",
    "start-prod": "docker run --rm --name ui -e NODE_ENV=prod -d -p 80:80 ui:latest",
    "setup-windows": "echo. >> C:\\Windows\\System32\\drivers\\etc\\hosts && echo 127.0.0.1 locals.ui.com >> C:\\Windows\\System32\\drivers\\etc\\hosts",
    "build": "ng build",
    "serve": "ng serve -o --port=4500",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.4",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "rxjs": "~6.6.0",
    "swiper": "^6.8.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "~10.2.3",
    "@angular/compiler-cli": "~10.2.4",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
